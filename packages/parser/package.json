{
  "name": "@wowarenalogs/parser",
  "author": "WoW Arena Logs",
  "license": "MIT",
  "version": "6.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "start": "npx tsdx watch",
    "build": "npx tsdx build",
    "build:parser": "npx tsdx build",
    "test": "npx tsdx test",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "prepare": "npm run build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "module": "dist/wow-combat-log-parser.esm.js",
  "size-limit": [
    {
      "path": "dist/wow-combat-log-parser.cjs.production.min.js",
      "limit": "200 KB"
    },
    {
      "path": "dist/wow-combat-log-parser.esm.js",
      "limit": "200 KB"
    }
  ],
  "eslintIgnore": [
    "node_modules",
    "dist",
    "build",
    "__generated__"
  ],
  "devDependencies": {
    "eslint-config-wowarenalogs": "*",
    "@types/lodash": "^4.14.186",
    "@types/uuid": "^8.3.4"
  },
  "dependencies": {
    "eventemitter3": "^4.0.7",
    "line-reader": "^0.4.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "md5": "^2.3.0",
    "moment-timezone": "^0.5.39",
    "rxjs": "6.6.6"
  }
}
